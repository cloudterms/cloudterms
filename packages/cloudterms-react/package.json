{
  "name": "@cloudterms/react",
  "module": "./dist/es/index.js",
  "type": "module",
  "scripts": {
    "build": "bun run build.css && bun run build.js",
    "build.css": "tailwindcss -i ./src/styles.css -o ./dist/index.css --minify",
    "build.js": "bunchee",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@cloudterms/js": "^0.0.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bunchee": "5.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  }
}
