{
	"name": "@cloudterms/react-vite-example",
	"type": "module",
	"scripts": {
		"build.app": "pnpm with-env tsc && vite build",
		"check": "tsc --noEmit && biome check .",
		"check.fix": "tsc --noEmit && biome check --write .",
		"dev": "PORT=3001 pnpm with-env  vite",
		"preview": "pnpm with-env vite preview",
		"with-env": "dotenv -e ../../.env -c --"
	},
	"dependencies": {
		"@cloudterms/react": "workspace:*",
		"dotenv-cli": "7.4.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"typescript": "^5.5.4",
		"vite": "^5.3.5"
	}
}
